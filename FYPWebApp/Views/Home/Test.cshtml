@{
    Layout = "~/Views/Shared/_dashboardlayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Test</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<form class="form-horizontal">
    <fieldset>

        
        <!-- Text input-->
        <div class="form-group">
            <label class="col-md-4 control-label" for="TestName">Test Name</label>
            <div class="col-md-4">
                <input id="TestName" name="TestName" type="text" placeholder="Blood Test" class="form-control input-md">

            </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
            <label class="col-md-4 control-label" for="LabName">Lab Name</label>
            <div class="col-md-4">
                <input id="LabName" name="LabName" type="text" placeholder="The Lab" class="form-control input-md">

            </div>
        </div>

        <!-- Textarea -->
        <div class="form-group">
            <label class="col-md-4 control-label" for="Description">Description</label>
            <div class="col-md-4">
                <textarea class="form-control" id="Description" name="Description"></textarea>
            </div>
        </div>

    </fieldset>
</form>
